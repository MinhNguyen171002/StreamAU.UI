<AuthorizeView>
    <Authorized>
        <span aria-hidden="true">@context.User.Identity.Name</span>
        <a href="/logout" class="logout"><i class="fa fa-sign-out"></i></a>
    </Authorized>
    <NotAuthorized>
        <a href="/register" class="register"><i class="fa fa-user-plus"></i></a>
        <a href="/login" class="login"><i class="fa fa-sign-in"></i></a>
    </NotAuthorized>
</AuthorizeView>
